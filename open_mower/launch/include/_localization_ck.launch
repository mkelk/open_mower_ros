<launch>

    <node pkg="mower_logic_ck" type="mower_odometry_ck" name="mower_odometry_ck" output="screen" required="true">
        <param name="imu_offset" value="-40.0"/>
        <param name="gps_antenna_offset" value="0.3"/>

        <param name="gps_filter_factor" value="0.3"/>
        <param name="magnetic_filter_factor" value="0.01"/>
        <param name="magnetic_offset_x" value="-32.0"/>
        <param name="magnetic_offset_y" value="-30.0"/>
    </node>

</launch>
